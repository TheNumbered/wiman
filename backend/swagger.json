      "post": {
        "tags": ["Notifications"],
        "summary": "Create a notification",
        "description": "Create a new notification for a specific user.",
        "operationId": "createNotificationPOST",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "type": "integer",
                    "description": "ID of the user receiving the notification"
                  },
                  "code": {
                    "type": "string",
                    "description": "Code identifying the type of notification"
                  },
                  "message": {
                    "type": "string",
                    "description": "The message content of the notification"
                  }
                },
                "required": ["userId", "code", "message"],
                "example": {
                  "userId": 123,
                  "code": "booking_cancelled",
                  "message": "Your booking has been cancelled"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Notification created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Notification created"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Invalid input"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
