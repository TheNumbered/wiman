          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "venueId": {
                    "type": "string"
                  },
                  "reportedBy": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "photos": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                },
                "required": ["venueId", "reportedBy", "description"]
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Maintenance issue reported",
            "content": {
              "application/json": {
